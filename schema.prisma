generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model templates {
  id     Int     @id(map: "template_pkey") @default(autoincrement())
  name   String  @db.VarChar
  secret String? @db.VarChar
}
